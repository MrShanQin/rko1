<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lizhi.rko.PhotoActivity">

    <RelativeLayout
        android:layout_margin="20dp"
        android:background="@mipmap/kuang_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout3">
        <EditText
            android:layout_margin="20dp"
            android:hint="输出文字"
            android:background="@null"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/editText2" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/editText2"
            android:layout_marginTop="105dp"
            android:layout_marginLeft="20dp"
            android:src="@mipmap/photo_photo" />
    </RelativeLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/relativeLayout3"
        android:layout_below="@+id/relativeLayout3"
        android:layout_marginStart="26dp"
        android:text="处理完成"
        android:background="@mipmap/pohto_buttom"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/relativeLayout3"
        android:layout_below="@+id/relativeLayout3"
        android:layout_marginEnd="26dp"
        android:text="问题申报"
        android:background="@mipmap/pohto_buttom"/>
</RelativeLayout>
